{
  "editor.foldingImportsByDefault": true,
  "editor.showFoldingControls": "always",
  "notebook.showFoldingControls": "always",
  "workbench.colorTheme": "Vue Theme",
  "workbench.preferredDarkColorTheme": "Abyss",
  "workbench.colorCustomizations": {


    "editor.lineHighlightBackground": "#003551",
    "editor.selectionBackground": "#004448",
    "editorGutter.foldingControlForeground": "#ff00ae",
    "editorBracketMatch.background": "#caff6e",
  },
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.minimumContrastRatio": 5,
  "material-icon-theme.activeIconPack": "none",
  "material-icon-theme.folders.theme": "specific",
  "editor.tokenColorCustomizations": {

    "[Abyss]": {
      "comments": "#5a6d83",
      "keywords": "#0074c7",
      "functions": "#2ee3ff",
      "variables": "#ee52ff",
      "numbers": "#00ff95",
      "strings": "#d7e8f5",
      "types": "#7d19e1",


      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },


    "[2077]": {
      "comments": "#5a6d83",
      "keywords": "#07a7aa",
      "functions": "#b8ffa3",
      "variables": "#ee52ff",
      "numbers": "#00ff95",
      "strings": "#92c9e4",
      "types": "#7d19e1",

      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },

    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*Dark*]": {

      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff",
          }
        }
      ]
    }
  },
  "editor.semanticTokenColorCustomizations": {

  },
  "editor.semanticHighlighting.enabled": false,
  "css.hover.documentation": false,
  "less.hover.documentation": false,
  "scss.hover.documentation": false
}
{
  "editor.foldingImportsByDefault": true,
  "editor.showFoldingControls": "always",
  "notebook.showFoldingControls": "always",
  "workbench.colorTheme": "Abyss",
  "workbench.preferredDarkColorTheme": "Abyss",
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#010157",
    "editor.selectionBackground": "#050646",
    "editorGutter.foldingControlForeground": "#ff0066",
    "editorBracketMatch.background": "#caff6e",
  },
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.minimumContrastRatio": 5,
  "material-icon-theme.activeIconPack": "none",
  "material-icon-theme.folders.theme": "specific",
  "editor.tokenColorCustomizations": {

    "[Abyss]": {
      "comments": "#5a6d83",
      "keywords": "#0074c7",
      "functions": "#2ee3ff",
      "variables": "#ee52ff",
      "numbers": "#00ff95",
      "strings": "#d7e8f5",
      "types": "#7d19e1",


      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },

    "[2077]": {
          "comments": "#5a6d83",
          "keywords": "#fff",
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },

    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*Dark*]": {

      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff",
          }
        }
      ]
    }
  },
  "editor.semanticTokenColorCustomizations": {

  },
  "editor.semanticHighlighting.enabled": false
}